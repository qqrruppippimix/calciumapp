<p id="notice"><%= notice %></p>

<h1>Listing English Books</h1>

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @english_books.each do |english_book| %>
      <tr>
        <td><%= english_book.price %></td>
        <td><%= english_book.description %></td>
        <td><%= link_to 'Show', english_book %></td>
        <td><%= link_to 'Edit', edit_english_book_path(english_book) %></td>
        <td><%= link_to 'Destroy', english_book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New English book', new_english_book_path %>
